<% include partials/_header %>


<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>IATA del Destino</th>
            <th>porcentaje en comparacion  a todos los vuelos</th>
    
      


        </tr>


    </thead>


    <tbody>

        <% if (data) { %>
        <% for (var i =0 ; i< data.length; i++) { %>
                <tr>
                    <td>
                        <%= data[i].destino %>
                    </td>
                    <td>
                        <%= data[i].porcentaje %> <p>%</p>
                    </td>
                   
                    
            
                </tr>


            <% } %>   
        <% } %>

    </tbody>
</table>



<% include partials/_footer %>